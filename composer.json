{
    "name": "artisanry/email-authentication",
    "description": "A password-less authentication system for Laravel",
    "keywords": [
        "laravel",
        "framework",
        "Laravel-Email-Authentication",
        "Laravel E-Mail Authentication"
    ],
    "license": "MIT",
    "authors": [
        {
            "name": "Brian Faust",
            "email": "hello@basecode.sh",
            "homepage": "https://basecode.sh"
        }
    ],
    "require": {
        "php": "^7.2"
    },
    "require-dev": {
        "graham-campbell/testbench": "^5.0",
        "mockery/mockery": "^1.0",
        "phpunit/phpunit": "^6.5"
    },
    "autoload": {
        "psr-4": {
            "Artisanry\\EmailAuth\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Artisanry\\Tests\\EmailAuth\\": "tests"
        }
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        },
        "laravel": {
            "providers": [
                "Artisanry\\EmailAuth\\EmailAuthServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
